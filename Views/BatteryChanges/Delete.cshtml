@model bs.Models.BatteryChange

@{
    ViewData["Title"] = "Eliminar cambio de baterías";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">

    <h3 class="mt-4">Eliminar cambio de baterías</h3>
    <h6 class="mt-4 text-danger">¿Está seguro que quiere eliminar este registro?</h6>
    <hr />
<div>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Agency)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Agency.AgencyName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Uninterruptible)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Uninterruptible.UpsModel)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.BatteryChangeDate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BatteryChangeDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.BatteryChangeComments)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BatteryChangeComments)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ModulesInst)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ModulesInst)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.BatteriesInst)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BatteriesInst)
        </dd>
    </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="BatteryChangeId" />
            <input type="submit" value="Eliminar" class="btn btn-danger" /> |
            <a asp-action="Index" class="btn btn-dark">Cancelar</a>
        </form>
</div>
</div>