<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="Proyecto de Práctica Profesional Supervisada, realizada en el Departamento de T.I
     del Banco de Occidente, Santa Rosa de Copán."/>
    <meta name="author" content="Cristian Andres Enamorado" />
    <title>@ViewData["Title"]</title>

    <link href="https://cdn.jsdelivr.net/npm/simple-datatables@latest/dist/style.css" rel="stylesheet" />   
    


    <link rel="shortcut icon" href="~/images/favicon.png" type="image/png" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/styles-template.css"/>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/bs.styles.css" asp-append-version="true" />

    </head>

<body class="sb-nav-fixed">
    <header>
      
        <!-- navbar -->

       
        <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
            <!-- Navbar Logo-->
            <div class="container col-md-3 mx-md-1">
            <a class="navbar-brand " asp-controller="BatteryChanges" asp-action="Main">
                <img src="~/images/logo277x35px.png" width="277" height="35" class="d-block">
            </a>
            <!-- Sidebar Toggle-->
            <a class="nav-link d-block" id="sidebarToggle" href="#!"><span data-feather="menu"></span></a>
            </div>
            <!-- Navbar Buscar
            <form class="d-none d-md-inline-block form-inline ms-auto me-0 me-md-3 my-2 my-md-0">
                <div class="input-group">
                    <input class="form-control" type="text" placeholder="Buscar..." aria-label="Buscar..." aria-describedby="btnNavbarSearch" />
                    <button class="btn btn-primary" id="btnNavbarSearch" type="button"><span class="p-1" data-feather="search"></span></button>
                </div>
            </form>-->
            <!-- Navbar-->
            
        </nav>



<!---->
       
    </header>

    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                <div class="sb-sidenav-menu">
                    <div class="nav">
                        <div class="sb-sidenav-menu-heading">Core</div>

                        <a class="nav-link" asp-controller="BatteryChanges" asp-action="Main">
                            <div class="sb-nav-link-icon text-light"><span data-feather="home"></span></div>
                            Inicio
                        </a>

                        <a class="nav-link" asp-controller="BatteryChanges" asp-action="Index">
                            <div class="sb-nav-link-icon text-light"><span data-feather="list"></span></div>
                            Lista de cambios de bateria
                        </a>
                        <a class="nav-link" asp-controller="BatteryChanges" asp-action="Create">
                            <div class="sb-nav-link-icon text-light"><span data-feather="plus-square"></span></div>
                            Agregar cambio de baterías
                        </a>
                        <div class="sb-sidenav-menu-heading">Configuración</div>

                        <a class="nav-link" asp-controller="Locations" asp-action="Index">
                            <div class="sb-nav-link-icon text-light"><span data-feather="map-pin"></span></div>
                            Localidades
                            
                        </a>
                      
                        <a class="nav-link" asp-controller="Agencies" asp-action="Index">
                            <div class="sb-nav-link-icon text-light"><span data-feather="dollar-sign"></span></div>
                            Agencias
                            
                        </a>
                       
                        
                        <a class="nav-link" asp-controller="Uninterruptibles" asp-action="Index">
                            <div class="sb-nav-link-icon text-light "><span data-feather="power"></span></div>
                            UPS
                        </a>
                       
                    </div>
                </div>
                
            </nav>
        </div>
        <div id="layoutSidenav_content">
            <main>

                <div class="container-fluid px-4">
                    @RenderBody()
                </div>




            </main>
             <footer class="py-4 bg-light mt-auto">
        <div class="container-fluid px-4">
            <div class="d-flex align-items-center justify-content-between small">
                
                        <div class="text-muted">&copy; @DateTime.Now.Year Designed by <a class="text-decoration-none" href="https://www.linkedin.com/in/cristian-andres-enamorado-sifontes-67ba99203" target="_blank">Cristian Enamorado</a></div>
                        <div><!--Espacio para texto al centro del footer--></div>
                        <div>
                             <a class="text-muted text-decoration-none">@DateTime.Now.ToLongDateString()</a>
                        </div>
            </div>
        </div>
    </footer>
        </div>
       
    </div>

 



    
   
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/feather/dist/feather.min.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="~/js/template-scripts.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.8.0/Chart.min.js" crossorigin="anonymous"></script>
    <script src="assets/demo/chart-area-demo.js"></script>
    <script src="assets/demo/chart-bar-demo.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/simple-datatables@latest" crossorigin="anonymous"></script>
    <script src="~/js/template-datatables-simple-demo.js"></script>

    <script>
        feather.replace()
    </script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

